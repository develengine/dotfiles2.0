#!/bin/sh

dotfiles=$HOME/devel/dotfiles2.0

cp /usr/share/xsessions/dwm.desktop $dotfiles
cp $HOME/.bashrc $dotfiles
cp $HOME/.config/.vimrc $dotfiles
cp $HOME/.config/compton.conf $dotfiles
# cp $HOME/.config/liferea/feedlist.opml $dotfiles
cp /usr/bin/onstart $dotfiles
cp $HOME/.config/nvim/init.vim $dotfiles
cp $HOME/.profile $dotfiles
cp /etc/profile $dotfiles
